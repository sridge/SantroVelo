<script>
//enter a customer name and return their info
//member is the JSON dictionary

function memberSearch() {
	
var firstLast, results;
var obj = { "bob smith": { "date":"11/13/12", "valid": "no"}};
var member = JSON.parse(obj)

FirstLast = document.getElementById("searchBox").value;

for(var i = 0; i < member.length; i++){
	if(var firstLast == member[i]) {
		results = member[i].date.valid;
	} else {
		results = "DNE";
	}
}
document.getElementById("print").innerHTML = results;
}
</script>

